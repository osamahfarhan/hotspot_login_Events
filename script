:set ($QUEUE->("ADDT")) do={
:global LASTQADDT;:if ($LASTQADDT=("$S-$N")) do={:delay 500ms;};:set $LASTQADDT ("$S-$N");
:global QUEUE;:global FIXSPQ;:global FIXSPD;
:if ([:len ($QUEUE->("ADDT")->("$S-$N"))]>0) do={:set ($QUEUE->("ADDT")->("$S-$N-NUM")) (($QUEUE->("ADDT")->("$S-$N-NUM"))+1);:if (($QUEUE->("ADDT")->("$S-$N-NUM"))>50) do={:set ($QUEUE->("ADDT")->("$S-$N-NUM")) 0;} else={:return ($QUEUE->("ADDT")->("$S-$N"));};};
