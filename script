:global PROFILEONLOGINLOAN;:set $PROFILEONLOGINLOAN do={
:global SPEEDS;:global OSAMA;:global ONLOGINU;:global SPEEDSFIX;:global SERVERUPTIME;:global ADDQULOGIN;:global SPEEDFORALL;:local ADDQU;:global FIRESTSEEN;:global ALLUSERSID;
:local U [:tostr $u];:local A [:tostr ("$a")];:local M [:tostr $m];
:local UM  ($U.(",").$M);:local UMA ($U.(",").$M.(",").$A);
:delay 1s;
:global KHADAMAT;:global ROUTER;:global COLLECT;
