:local y [($OSAMA->("NUM2YEAR")) N=$N];
:local sp $S1;:if ([:len $sp]=0) do={:set $sp ("/");};
:if ("$OPTION"="SPLIT") do={:set ($OSAMA->("NUM2FILENAME")->("$ca")) ("$y","$m","$d");:return ("$y","$m","$d");}
:set ($OSAMA->("NUM2FILENAME")->("$ca")) ("$y$sp$m$sp$d");
:return ("$y$sp$m$sp$d");
};
:set ($OSAMA->("ANY2ARR")) do={
:global OSAMA;
:local A [:toarray ""];
:if ([:len $S]=0) do={:return $A;};
:if ([:typeof $S]="array") do={:foreach n,v in=$S do={:if ([:typeof $n]="num") do={:set $A [($OSAMA->("ANY2ARR")) S=[:pick $S 1 [:len $S]]];} else={:set ($A->("$n")) [($OSAMA->("ANY2ARR")) S=$v];};:return $A;};};
:set ($A->("$S")) [:toarray ""];
:return $A;
};
:set ($OSAMA->("DATE2ARR")) do={
:global OSAMA;
:if ([:len ($OSAMA->("DATE2ARR")->("$Date"))]>0) do={:if ([:len ($OSAMA->("DATE2ARR")->("$Time"))]>0) do={:return (($OSAMA->("DATE2ARR")->("$Date")),($OSAMA->("DATE2ARR")->("$Time")));};};
:if ($Date~"[0-9]{4}-[0-9][0-9]-") do={:set $Date [($OSAMA->("DATE2V6")) Date=$Date];};
:if ([:len $Time]=0) do={:if ($Date~("[:]")) do={:local f1 [:find $Date ("/")];:local f2 [:find $Date (":")];:set $Date [:pick $Date ($f1-3) ($f1+8)];:set $Time [:pick $Date ($f2-2) ($f2+6)];} else={:set $Time [/sys clock get time];};};
:local T  [:pick ([:tostr $Time].("00:00:00")) 0 8];
:local D  [:pick $Date 0 11];:if ($D~("[0-9]{4}-[0-9][0-9]-")) do={:set $D [($OSAMA->("DATE2V6")) Date=$D];};
:if ([:len ($OSAMA->("DATE2ARR")->("$D"))]>0) do={:if ([:len ($OSAMA->("DATE2ARR")->("$T"))]>0) do={:return (($OSAMA->("DATE2ARR")->("$D")),($OSAMA->("DATE2ARR")->("$T")));};};
:if ($Date~("[/]")) do={:local f1 ([:find $D ("/")]+0);:local f2 ([:find $D ("/") $f1]+0);:local m [:pick $D 0 $f1];:local d [:pick $D ($f1+1) $f2];:local y [:pick $D ($f2+1) ($f2+5)];:set ($OSAMA->("DATE2ARR")->("$D")) ("$y","$m","$d");};
:if ($Time~("[:]")) do={:local f1 ([:find $T (":")]+0);:local f2 ([:find $T (":") $f1]+0);:local h [:pick $T 0 $f1];:local m [:pick $T ($f1+1) $f2];:local s [:pick $T ($f2+1) ($f2+3)];:set ($OSAMA->("DATE2ARR")->("$T")) ("$h","$m","$s");};
:return (($OSAMA->("DATE2ARR")->("$D")),($OSAMA->("DATE2ARR")->("$T")));
};
:set ($OSAMA->("NUM2DATE")) do={
:global OSAMA;
:local L [:len $N];:local n ([:tonum ("$N")]+0);:if ($L>1 && $n=0 && ("$N"!="0")) do={:log warning ("NUM2DATE Error N=($N) return=$n");:error ("NUM2DATE Error N=($N) return=$n");};
