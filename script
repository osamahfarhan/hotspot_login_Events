:global QUEUE;:set $QUEUE [:toarray ""];
:set ($QUEUE->("TREE/ADD")) do={:local ID;:if ([:len $N]=0) do={:return (" ");};:do {:set $ID [/queue tree add max-limit=$ML name=$N packet-mark=$PM parent=global priority=7 queue=$Q]} on-error={};:return $ID;};
